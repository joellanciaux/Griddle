{
  "name": "griddle-react",
  "version": "1.0.0-alpha.40",
  "description": "A fast and flexible grid component for React",
  "keywords": [
    "react-component",
    "grid",
    "react",
    "pagination",
    "sort"
  ],
  "main": "build/griddle.js",
  "scripts": {
